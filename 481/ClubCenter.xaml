    <Grid>
        <TextBox Background="#6D6D6D" Foreground="White" HorizontalAlignment="Left" Height="23" Margin="82,143,0,0" TextWrapping="Wrap" Text="Search: " VerticalAlignment="Top" Width="400" RenderTransformOrigin="0.498,0.087" KeyDown="CompKey">
            
        </TextBox>
            <Button Background="#4D4D4D" Foreground="White" Content="Information Security Club" HorizontalAlignment="Left" VerticalAlignment="Top" Width="320" Height="100" Click="Info_Click" Margin="82,38,0,0">
